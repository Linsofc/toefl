{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/admin.html",
            "dest": "/api/index.js"
        },
        {
            "src": "/dashboard.html",
            "dest": "/api/index.js"
        },
        {
            "src": "/login.html",
            "dest": "/api/index.js"
        },
        {
            "src": "/admin_login.html",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
